ifeq (`xeno-config --version`, 2.4.4)
TARGET_CFLAGS   = `xeno-config --xeno-cflags` -DXENO_POSIX -D_POSIX_SOURCE -D_GNU_SOURCE -m32
TARGET_LDFLAGS  = `xeno-config --xeno-ldflags` -m32
else
TARGET_CFLAGS   = `xeno-config --skin native --cflags` -DXENO_POSIX -D_POSIX_SOURCE -D_GNU_SOURCE -m32
TARGET_LDFLAGS  = `xeno-config --skin native --ldflags` -m32
endif

SIMULINK_LDFLAGS  = -lm
CC              = gcc
LD              = ld
GPROF           = gprof


ifdef USE_GPROF
TARGET_LDFLAGS+=-pg
endif


target-clean:

target-objects:
